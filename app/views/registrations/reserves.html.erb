<div style="float: right; color: #EEEEEE; margin-bottom: -1px; font-size: 18pt; font-weight: bold;">
    RESERVER
</div>
<table class="table table-striped">
    <thead class="head">
        <tr>
            <th>Förnamn</th>
            <th>Efternamn</th>
            <th>Årskurs</th>
            <th>Kommentar</th>
        </tr>
    </thead>
    <tbody class="registrations">

        <%= render :partial => 'table_row', :collection => @reserves %>

    </tbody>
</table>
<div class="pagebreak"><%= will_paginate @reserves %></div>


<script type="text/javascript">
    $("[rel=tooltip]").tooltip({placement: 'right'});

    $('tr').click(function () {
        window.location.href = '<%= project_registrations_path(@project) %>/' + $(this).attr('id')
    });   
</script>